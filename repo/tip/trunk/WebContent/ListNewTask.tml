<t:layout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
	List Of New Task
    <t:grid  source="taskItems" row="taskItem" pagerPosition="both" model="model">

		<t:parameter name="castVoteCell">
	 	<t:if test="visit.user">
	 		<t:if test="visit.user.ableToVote">
				<t:actionlink t:id="castVote" context="taskItem.id">Cast Vote</t:actionlink>
	        </t:if>
		</t:if>
		</t:parameter>    	

    </t:grid>        
</t:layout>